{
    "name": "vfac/envdevhome",
    "description": "A homepage for EnvDev project",
    "keywords": ["docker", "docker-toolbox", "devtools", "web", "environment", "developer-tools", "development", "php", "ssl", "envdev"],
    "homepage": "http://github.com/vfalies/envdev",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent FaliÃ¨s",
            "email": "vincent@vfac.fr",
            "homepage": "https://vfac.fr/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "": "src/classes/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
